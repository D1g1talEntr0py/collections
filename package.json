{
  "name": "@d1g1tal/collections",
  "version": "0.0.3",
  "description": "JavaScript Collections ES Modules",
  "type": "module",
  "types": "collections.d.ts",
  "exports": {
    ".": {
      "import": "./collections.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/src",
    "/dist",
    "collections.js",
    "collections.d.ts"
  ],
  "scripts": {
    "lint": "eslint --ext .js --fix --ignore-path .gitignore .",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D1g1talEntr0py/collections.git"
  },
  "keywords": [
    "Collections",
    "ESModules"
  ],
  "author": "Jason DiMeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/D1g1talEntr0py/collections/issues"
  },
  "homepage": "https://github.com/D1g1talEntr0py/collections#readme",
  "devDependencies": {
    "esbuild": "^0.15.11",
    "eslint": "^8.25.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-jsdoc": "^39.3.6",
    "jest": "^29.2.0"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "Firefox ESR",
    "not dead",
    "not ios_saf < 15",
    "not op_mini all"
  ],
  "jest": {
    "verbose": true,
    "transform": {},
    "coverageDirectory": "./tests/coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
