{
  "name": "@d1g1tal/collections",
  "version": "0.2.0",
  "description": "JavaScript Collections ES Modules",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./*": "./src/*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/src",
    "/dist",
    "index.js"
  ],
  "scripts": {
    "lint": "eslint --ext .js --fix --ignore-path .gitignore .",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D1g1talEntr0py/collections.git"
  },
  "keywords": [
    "Collections",
    "ESModules"
  ],
  "author": "Jason DiMeo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/D1g1talEntr0py/collections/issues"
  },
  "homepage": "https://github.com/D1g1talEntr0py/collections#readme",
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-jsdoc": "^46.2.6",
    "jest": "^29.5.0"
  },
  "browserslist": [
    "defaults",
    "not ios_saf < 15",
    "not op_mini all"
  ],
  "jest": {
    "verbose": true,
    "transform": {},
    "coverageDirectory": "./tests/coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  }
}
